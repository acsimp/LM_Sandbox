              <% if(!currentUser){ %>
              <li class="nav-item">
                <a class="nav-link <%= typeof page !== 'undefined' && page === 'register' ? 'active' : '' %> " href="/register">Sign Up</a>
              </li>              
              <li class="nav-item">
                <a class="nav-link <%= typeof page !== 'undefined' && page === 'login' ? 'active' : '' %> " href="/login">Login</a>
              </li>
              <% }else{ %>
              <li>
                <a class="nav-link" href="/users/<%= currentUser._id %>"><figure class="avatar avatar" data-initial="A"></figure> <span id="username"> <%= currentUser.username %></span></a>
              </li>
