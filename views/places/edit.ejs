                                <script>
                                    
                                    var els = document.getElementsByClassName("hour_num");

                                    Array.prototype.forEach.call(els, function(el) {
                                        // Do stuff here
                                        
                                        var number_id, string_id = String;
                                        var string = document.getElementById(el.id);
                                        string_id = String(string.id) + "_str";
                                        number_id = String(string.id);
                                        // console.log(string_id + ": " + typeof(string_id));
                                        // console.log(number_id + ": " + typeof(number_id));
                                        
                                        var string = document.getElementById(string_id);
                                        var number = document.getElementById(number_id);
                                        // console.log(string, number)
                                        string.addEventListener("input", function() {
                                            var time,hr,min,minutes;
                                            time = string.value.split(" ");
                                            time = time[0].split(":");
                                            minutes = (parseInt(time[0])*60) + parseInt(time[1]);
                                            number.value = minutes;
                                    });
                                    });
                                    </script>
                                    
                                    <script>
                                        var sevenDays = ['hours_mon', 'hours_tue','hours_wed','hours_thu','hours_fri','hours_sat', 'hours_sun'];
                                        
                                        Array.prototype.forEach.call(sevenDays, function(day) {
                                        window.onload = toggleSelect(day); // to disable select on load if needed
                                        });

                                        function toggleSelect(day)
                                        {
                                            var x,closed,close_str,allDay,open_str,open,close = String;
                                            x = String(day);
                                            closed = x + "_closed";
                                            allDay = x + "_allDay";
                                            close_str = x + "_close_str";
                                            open_str = x + "_open_str";
                                            close = x + "_close";
                                            open = x + "_open";
                                            // console.log(x + closed + open + open_str + close_str + allDay + close);
                                            if (x !== "") {
                                          var closedIsChecked = document.getElementById(closed).checked;
                                          var allDayIsChecked = document.getElementById(allDay).checked;
                                          if(allDayIsChecked || closedIsChecked) {
                                          document.getElementById(open_str).disabled = true;
                                          document.getElementById(close_str).disabled = true;
                                          document.getElementById(open_str).value = "";
                                          document.getElementById(close_str).value = "";
                                          document.getElementById(open).value = "";
                                          document.getElementById(close).value = "";
                                          } else {
                                              document.getElementById(open_str).disabled = false;
                                              document.getElementById(close_str).disabled = false;
                                          }
                                        }}
                                       
                                    </script>
                     
